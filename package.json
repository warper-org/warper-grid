{
  "name": "@itsmeadarsh/warper-grid",
  "version": "1.2.2",
  "description": "A professional, feature-rich React data grid with TypeScript support, modular plugins, and beautiful UI",
  "author": "Adarsh Gourab Mahalik <e2vylu0d0@mozmail.com>",
  "license": "Proprietary",
  "homepage": "https://grid.warper.tech",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warper-org/warper-grid.git"
  },
  "bugs": {
    "url": "https://github.com/warper-org/warper-grid/issues"
  },
  "keywords": [
    "react",
    "data-grid",
    "datagrid",
    "table",
    "grid",
    "typescript",
    "react-table",
    "excel",
    "spreadsheet",
    "virtualized",
    "sorting",
    "filtering",
    "pagination",
    "editing"
  ],
  "type": "module",
  "main": "./dist/warper-grid.cjs",
  "module": "./dist/warper-grid.js",
  "types": "./dist/warper-grid/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/warper-grid/index.d.ts",
      "import": "./dist/warper-grid.js",
      "require": "./dist/warper-grid.cjs"
    },
    "./styles": "./dist/warper-grid.css"
  },
  "files": [
    "dist/warper-grid.js",
    "dist/warper-grid.js.map",
    "dist/warper-grid.cjs",
    "dist/warper-grid.cjs.map",
    "dist/warper-grid",
    "dist/warper_wasm*.js",
    "dist/warper_wasm*.cjs",
    "dist/warper_wasm*.map",
    "dist/warper_wasm_bg.wasm",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode lib && tsc -p tsconfig.lib.json",
    "build:site": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build:lib"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
